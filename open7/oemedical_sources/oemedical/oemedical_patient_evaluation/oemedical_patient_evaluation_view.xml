		<record id="view_oemedical_patient_evaluation_form" model="ir.ui.view">
			<field name="name">oemedical.patient.evaluation.form</field>
			<field name="model">oemedical.patient.evaluation</field>
			<field name="priority" eval="8" />
			<field name="arch" type="xml">
				<form string="Oemedical Patient Evaluation" version="7.0">
				<sheet>
					<group co="8">
						<group co="6">
							<field name="patient_id" />
							<field name="doctor" />
							<!--field name="specialty"/-->
							<field name="evaluation_date" readonly="0"/>
						</group>
						
						<group co="2">
							<div class="oe_button_box oe_right">
							<button name="button_print_official_report" string="Print Official Report" type="object" class="oe_highlight oe_readonly"/>
							</div>
						</group>
					</group>
				   
				   <notebook>
				    <page string="Signos Vitales">
					<group colspan="4" col="4">
					<field name="pes_info" />
					<field name="est_info" />
					<field name="pat_info" />
					<field name="fqr_info" />
					<field name="ppm_info" />
					<field name="tem_info" />
					<field name="osat"/>
					</group>
					</page>
				    <page string="EvaluaciÃ³n">
                        <group colspan="4" col="4">
                        <label for="mdc_info"/>
                        <field name="mdc_info" nolabel="1" colspan="4"/>
                        <newline/>
                        <label for="ena_info"/>
                        <field name="ena_info" nolabel="1" colspan="4"/>
                        <newline/>
                        <label for="rxl_complaint"/>
                        <field name="rxl_complaint" nolabel="1" colspan="4"/>
                        <newline/>
                        <label for="revision_organos"/>
                        <field name="revision_organos" nolabel="1" colspan="4"/>
                        <newline/>
                         <field name="org_scp" />
                         <field name="org_ssp" />
                         <field name="res_cp" />
                         <field name="res_sp" />
                        <newline/>
                         <field name="car_vcp" />
                         <field name="car_vsp" />
                         <field name="dig_cp" />
                         <field name="dig_sp" />
                        <newline/>
                         <field name="gen_cp" />
                         <field name="gen_sp" />
                         <field name="uri_cp" />
                         <field name="uri_sp" />
                        <newline/>
                         <field name="mus_ecp" />
                         <field name="mus_esp" />
                         <field name="end_cp" />
                         <field name="end_sp" />
                         <newline/>
                         <field name="hmo_lcp" />
                         <field name="hmo_lsp" />
                         <field name="nrv_cp" />
                         <field name="nrv_sp" />
                         <newline/>
                         <field name="des_eva" colspan="4"/>
                         <newline/>
                        <field name="cab_cp" />
                        <field name="cab_sp" />
                        <field name="cue_cp" />
                        <field name="cue_sp" />
                        <newline/>
                        <field name="trx_vcp" />
                        <field name="trx_vsp" />
                        <field name="abd_cp" />
                        <field name="abd_sp" />
                        <newline/>
                        <field name="pel_cp" />
                        <field name="pel_sp" />
                        <field name="ext_cp" />
                        <field name="ext_sp" />
                        <newline/>
                        <field name="des_erg" colspan="4"/>
                        <newline/>
                        <label for="diagnosis"/>
					    <field name="diagnosis" nolabel="1" colspan="4"/>
					    <newline/>
                        <label for="definitive_diagnosis"/>
                        <field name="definitive_diagnosis" nolabel="1" colspan="4"/>
                        <newline/>
                        <label for="eac_info"/>
                        <field name="eac_info" nolabel="1" colspan="4" />
                        <newline/>
                        <label for="derived_to"/>
                        <field name="derived_to" nolabel="1" colspan="4"/>
                        <newline/>
                        <label for="directions"/>
                        <field name="directions" nolabel="1" colspan="4"/>
                        </group>
				      </page>
				      <page string="Evoluciones">
				      	<field name="evolution_ids" nolabel="1"/>
				      </page>					 
				    </notebook>
				</sheet>
				</form>
			</field>
		</record>

		<record id="view_oemedical_patient_evaluation_search" model="ir.ui.view">
