    def _get_list_words(self, data, size):
        if not data: return []
        text = data.replace('\n', ' ')
        if len(text) <= size: return [text]
        
        text = text.split(' ')
        was_pushed = False
        word_list = []
        temp = ''
        
        for x in xrange(len(text)):
            if len(temp) + len(text[x]) + 1 <= size:
                if text[x] != ' ': temp += text[x] + ' '
            else:
                word_list.append(temp)
                was_pushed = True
                temp = ''
            
            if x == len(text) - 1: word_list.append(temp)
        return word_list
