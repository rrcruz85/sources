<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="appointment_notification_config" model="oemedical.appointment.notification">
            <field name="notify_before">1</field>
            <field name="unit_time">hour</field>
            <field name="first_monthly" eval="True" />
            <field name="third_monthly" eval="True" />
            <field name="sixth_monthly" eval="True" />
            <field name="nineth_monthly" eval="True" />
            <field name="yearly" eval="True" />
            <field name="show_notification">1</field>
            <field name="show_unit_time">minute</field>
            <field name="company_id" ref="base.main_company"/>
        </record>
    </data>
</openerp>
