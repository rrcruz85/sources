		<record id="view_oemedical_bariatric_evolution_search" model="ir.ui.view">
			<field name="name">oemedical.bariatric.evolution.search</field>
			<field name="model">oemedical.bariatric.evolution</field>
			<field name="priority" eval="8" />
			<field name="arch" type="xml">
				<search string="Bariatric Evolution">
					<field name="cir_tip" />
					<field name="cir_date" />
					<field name="evo_date" />
					<!--field name="eva_value" /-->
					<!--field name="directions" /-->
					<newline />
					<group expand="0" string="Group By...">
						<filter string="Tipo cirugia:" domain="[]"
							context="{'group_by':'cir_tip'}" />
						<filter string="Doctor" domain="[]"
							context="{'group_by':'doctor'}" />
					</group>
				</search>
			</field>
		</record>
		
